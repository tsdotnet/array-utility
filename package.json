{
  "name": "@tsdotnet/array-utility",
  "version": "1.0.28",
  "description": "A small collection of useful array functions.",
  "author": "electricessence",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/arrayUtility.js",
      "require": "./dist/cjs/arrayUtility.js",
      "types": "./dist/arrayUtility.d.ts"
    }
  },
  "types": "dist/arrayUtility.d.ts",
  "main": "dist/cjs/arrayUtility.js",
  "module": "dist/esm/arrayUtility.js",
  "umd:main": "dist/umd/arrayUtility.js",
  "unpkg": "dist/umd/arrayUtility.js",
  "jsdelivr": "dist/umd/arrayUtility.js",
  "keywords": [
    "array",
    "indexOf",
    "contains",
    "replace",
    "updateRange",
    "clearEach",
    "register",
    "findIndex",
    "forEach",
    "applyTo",
    "removeIndex",
    "remove",
    "repeat",
    "range",
    "rangeUntil",
    "distinct",
    "flatten",
    "init",
    "copy",
    "copyTo"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "tsc -p tsconfig.umd.json",
    "build:types": "cpy \"src/**/*.d.ts\" dist --parents && tsc -p tsconfig.types.json",
    "build": "pnpm i && pnpm run clean && concurrently \"pnpm:build:*\"",
    "bump": "pnpm run precommit && pnpm version patch",
    "docs": "rimraf docs && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && pnpm run build && pnpm run lint && pnpm run validate && pnpm run test && git status -s",
    "prepublishOnly": "pnpm run build && pnpm run validate && pnpm run test",
    "preversion": "pnpm run lint && pnpm run test",
    "postversion": "git push && git push --tags && pnpm run docs && git add -A && git commit -m \"Updated docs.\" docs && git push",
    "test": "tsx node_modules/mocha/bin/_mocha tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/array-utility.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/array-utility/issues"
  },
  "homepage": "https://github.com/tsdotnet/array-utility#readme",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tsdotnet/common-interfaces": "^1.0.4",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "chai": "^5.2.1",
    "concurrently": "^9.2.0",
    "cpy-cli": "^6.0.0",
    "eslint": "^9.33.0",
    "mocha": "^11.7.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.4",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "@tsdotnet/array-copy": "^1.2.1",
    "@tsdotnet/array-init": "^1.2.1",
    "@tsdotnet/compare": "^1.5.1",
    "@tsdotnet/exceptions": "^1.2.2",
    "@tsdotnet/integer": "^1.2.1",
    "@tsdotnet/type": "^1.2.1",
    "tslib": "^2.8.1"
  },
  "pnpm": {
    "overrides": {
      "path-parse@<1.0.7": ">=1.0.7",
      "hosted-git-info@<2.8.9": ">=2.8.9",
      "y18n@=4.0.0": ">=4.0.1",
      "lodash@<4.17.20": ">=4.17.20",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "lodash@<4.17.21": ">=4.17.21",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "word-wrap@<1.2.4": ">=1.2.4",
      "minimist@>=1.0.0 <1.2.6": ">=1.2.6",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "get-func-name@<2.0.1": ">=2.0.1",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "braces@<3.0.3": ">=3.0.3"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
